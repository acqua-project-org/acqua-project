<?xml version="1.0" encoding="UTF-8" standalone="no"?><ACQUA_CONFIG_FILE>
	<!-- Timeout (in seconds) for the pings. -->
	<timeout value="3"/>
	<!-- Number of pings per campain (per landmark). -->
	<number_of_pings value="3"/>
	<!-- Landmarks to analyze. -->
	<landmarks></landmarks>
	<!-- Period of campains (in milliseconds). -->
	<ife_execution_period value="0"/> 
	<!-- Amplitude of the anomaly (in milliseconds) to detect. -->
	<anomaly_detection_parameter1 value="14.0"/>
	<!-- Lower index threshold to start judging as anomaly. Between 0 and 1. -->
	<anomaly_detection_parameter2 value="0.1"/>
	<!-- Upper index threshold to start judging as normal (after anomaly). Between 0 and 1. -->
	<anomaly_detection_parameter3 value="0.9"/>
	<!-- The other parameters are not being used currently bu AnomalyDetectionBernoulli. -->
	<anomaly_detection_parameter4 value="5.0"/>
	<anomaly_detection_parameter5 value="5.0"/>
	<!-- Invisible mode. -->
	<!-- Number of campains to perform (only for invisible mode). -->
	<number_of_loops value="2000"/>
	<!-- Default name for the session file (only used in invisible mode). -->
	<session_filename> session.dat </session_filename>
</ACQUA_CONFIG_FILE>
